    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "./dist",
    "allowJs": true,
    "target": "es5",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "include": [
    "./core/**/*", 
    "./exchange/**/*", 
    "./importers/**/*", 
    "./plugins/**/*", 
    "./strategies/**/*", 
    "./gekko.js", 
    "./config.js", 
    "./package.json"]
}